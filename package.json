{
  "name": "aqa-wdio-3",
  "type": "module",
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@faker-js/faker": "^9.3.0",
    "@stylistic/eslint-plugin-js": "^4.2.0",
    "@types/ajv": "^0.0.5",
    "@types/axios": "^0.9.36",
    "@types/chai": "^5.0.1",
    "@types/dotenv": "^6.1.1",
    "@types/eslint__js": "^8.42.3",
    "@types/lodash": "^4.17.13",
    "@types/mocha": "^10.0.10",
    "@types/rimraf": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^8.14.0",
    "@typescript-eslint/parser": "^8.14.0",
    "@wdio/allure-reporter": "^9.11.0",
    "@wdio/cli": "^9.12.0",
    "@wdio/local-runner": "^9.12.0",
    "@wdio/mocha-framework": "^9.11.0",
    "@wdio/spec-reporter": "^9.11.0",
    "@wdio/types": "^9.10.1",
    "@wdio/visual-service": "^6.3.3",
    "cross-env": "^7.0.3",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.6",
    "globals": "^15.12.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "3.5.3",
    "ts-node": "^10.9.2",
    "typescript-eslint": "^8.14.0",
    "wdio-custom-commands-matchers": "^1.0.2",
    "wdio-intercept-service": "^4.4.1"
  },
  "scripts": {
    "test": "npx wdio run ./wdio.conf.ts",
    "test:ui-products": "npm run test -- --suite ui_products",
    "test:single": "cross-env HEADLESS=true npm run test -- --suite single",
    "test:ui-simple": "npm run test -- --suite ui_simple",
    "test:api-products": "npm run test -- --suite api_products",
    "test:testp": "npm run test -- --tag '@TEST'",
    "test:smoke": "npm run test -- --tag @SMOKE",
    "test:regression": "npm run test -- --tag @REGRESSION",
    "test:smoke-regression": "npm run test -- --tag @SMOKE --tag @REGRESSION",
    "test:global-setup": "npm run test -- --tag @GLOBAL_SETUP",
    "report:generate": "allure generate allure-results -o allure-report --clean",
    "report:open": "npm run report:generate && allure open allure-report",
    "lint:check": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:check:staged": "eslint --cache --cache-location node_modules/.cache/eslint/",
    "lint:fix:staged": "eslint --fix --cache --cache-location node_modules/.cache/eslint/",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "code-quality": "npm run lint:check && npm run format",
    "code-quality:fix": "npm run lint:fix && npm run format:fix",
    "wdio": "wdio run ./wdio.conf.ts",
    "test:api:smoke": "npx mocha --grep '@SMOKE' src/api/tests/**/*.test.ts",
    "test:api:regression": "npx mocha --grep '@REGRESSION' src/api/tests/**/*.test.ts",
    "test:api:smoke-regression": "npx mocha --grep '@SMOKE|@REGRESSION' src/api/tests/**/*.test.ts",
    "test:api:no-smoke": "npx mocha --grep '(?!@SMOKE).*' src/api/tests/**/*.test.ts",
    "test:api:single": "npx mocha src/api/tests/products/getAll.test.ts",
    "test:api:products": "npx mocha src/api/tests/products/**/*.test.ts"
  },
  "dependencies": {
    "ajv": "^8.17.1",
    "allure-commandline": "^2.32.0",
    "allure-mocha": "^3.2.1",
    "axios": "^1.7.9",
    "chai": "^5.2.0",
    "dotenv": "^16.5.0",
    "lodash": "^4.17.21",
    "logger-vlaqa": "^1.0.5",
    "mocha": "^11.1.0",
    "rimraf": "^6.0.1",
    "webdriverio": "^9.12.0"
  },
  "lint-staged": {
    "*.{js,ts,mjs}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  }
}
